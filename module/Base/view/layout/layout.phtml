    <!-- Interaction Scripts -->
    <script>
        // New Header Mobile Menu Script
        const hamburgerBtn = document.querySelector('.auto-align-hamburger');
        const closeBtn = document.querySelector('.auto-align-close-btn');
        const mobileNav = document.querySelector('.auto-align-nav-mobile');
        const body = document.body;

        if (hamburgerBtn && closeBtn && mobileNav) {
            hamburgerBtn.addEventListener('click', () => {
                mobileNav.classList.add('auto-align-is-open');
                body.style.overflow = 'hidden';
            });

            closeBtn.addEventListener('click', () => {
                mobileNav.classList.remove('auto-align-is-open');
                body.style.overflow = 'auto';
            });
        }

        // Scroll to top function
        function scrollToTop() {
            window.scrollTo({
                top: 0,
                behavior: 'smooth'
            });
        }
    </script>
    <script defer src="https://cdn.jsdelivr.net/npm/alpinejs@3.x.x/dist/cdn.min.js"></script>
